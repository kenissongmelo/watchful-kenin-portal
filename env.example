# KeninDuty Configuration
# =======================

# Server Configuration
# -------------------
KENINDUTY_PORT=7007
NODE_ENV=development

# Logging Configuration
# --------------------
LOG_LEVEL=info
LOG_FILE=keninduty-logs.json

# CORS Configuration
# ------------------
CORS_ORIGIN=http://localhost:3000

# Timeout Configuration
# --------------------
REQUEST_TIMEOUT=5000

# Rate Limiting
# -------------
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Provider Integration (Optional)
# ------------------------------
# New Relic
NEWRELIC_API_KEY=your_newrelic_api_key
NEWRELIC_ACCOUNT_ID=your_newrelic_account_id

# Datadog
DATADOG_API_KEY=your_datadog_api_key
DATADOG_APP_KEY=your_datadog_app_key

# Grafana
GRAFANA_URL=https://your-grafana-instance.com
GRAFANA_TOKEN=your_grafana_token

# Call System Integration (Optional)
# ---------------------------------
# Twilio (example)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Security (Optional)
# ------------------
JWT_SECRET=your_jwt_secret_key
SESSION_SECRET=your_session_secret 